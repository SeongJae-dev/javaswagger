<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="follower">
  <select id="selectAll" resultType="followerVo">
    select * from Follower
  </select>
  
  <insert id="insert">
  	insert into follower values(#{user_ID},#{follower_ID})
  </insert>
  
  <delete id="delete">
  	delete follower where follower_ID = #{follower_ID}
  </delete>
</mapper>